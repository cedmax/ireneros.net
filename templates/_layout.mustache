<!doctype html>
<!--[if lt IE 7 ]><html class="ie ie6" lang="en"> <![endif]-->
<!--[if IE 7 ]><html class="ie ie7" lang="en"> <![endif]-->
<!--[if IE 8 ]><html class="ie ie8" lang="en"> <![endif]-->
<!--[if (gte IE 9)|!(IE)]><!--><html lang="en"> <!--<![endif]-->
	<head>
		{{>head}}
	</head>
<body>

<script type="text/javascript">
	document.write("<div id='sitePreloader'><div id='preloaderImage'><img src='images/site_preloader.gif' alt='Preloader' /></div></div>");
</script>

<div class="container">
	
	{{>header}}
		
	<div id="overview" class="sixteen columns">
		<h3>{{{quote}}}</h3>
		<hr />
	</div>

	{{#sections}}
		<div class="project eight columns">
			<div class="projectThumbnail">
				<a href="{{url}}" class="projectThumbnailHover">
					<h4>{{title}}</h4>
					<h5>{{description}}</h5>
				</a>
				<img src="{{thumb}}" alt="{{title}}" class="thumbnailImage" />
			</div>
		</div>
	{{/sections}}
	
	{{>footer}}
</body>
</html>